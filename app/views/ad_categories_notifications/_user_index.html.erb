<div id="ad_categories_notifications">
  <% @ad_categories_notifications = AdCategoriesNotification.search(params[:search])
     @ad_categories_notifications = @ad_categories_notifications.where("user_id = ? ", current_user.id)
  %>
  <% @ad_categories_notifications = @ad_categories_notifications.paginate(:per_page => 8, :page => params[:page]) %>
  <%= render :partial => 'ad_categories_notifications/ad_categories_notifications' , :locals => {:index_scope => "creator" , :ad_categories_notifications => @ad_categories_notifications }%>
</div>
