<%= form_tag shopping_malls_path, :method => 'get', :id => "mall_search"  do %>
    <%= text_field_tag :search, params[:search], :class => "search_text_field",
                       :style=> "background-color: #ffffff; height:24px; width:260px; top:0px; " %>
    <input type="submit"  style="background-color: #E8E7E2; ;height:32px; width: 100px; top:-5px; left: -3px; font-weight: bolder; border-radius: 8px;" value="GO"/>
<% end %>


